@for (int i = 0; i < Model.animals.Count; i++)
{
    AnimalDiseaseQueryWebApp.Models.Animal animal = Model.animals[i];

    <div class="row">
        <div class="col-md-2">
            <h3>  @animal.Name @animal.Sex @animal.Age </h3>
        </div>
    </div>



    foreach (var pd in Model.GetAllPriorsForAnimal(animal))
    {


    <div class="row">
        <div class="col-md-2">
            @pd.Id
        </div>

        <div class="col-md-2">
            @pd.Disease.Name
        </div>

        <div class="col-md-2">
        @Model.GetProbabilityForDisease(pd.Disease, animal)
            
        </div>



    </div>

    }
    }
