@model DiagnosticDataVisualiser.Models.AdminPageViewModel
@{
    ViewBag.Title = "Index";
}

<h2>Data Visualisation Admin Page</h2>

<div class="col-md-6">
    <h3>Welcome @Model.currentUser</h3>
</div>

@foreach (var user in Model.registeredUsers)
{
<div class="row">
    <div class="col-md-4">
        @user.Email
    </div>
    <div class="col-md-4">
        @user.Role
    </div>
</div>
}

